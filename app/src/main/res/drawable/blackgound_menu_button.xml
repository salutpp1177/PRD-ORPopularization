<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#23B5E2"
                android:endColor="#3AEEA1"
                />
            <corners
                android:radius="30dp"
                />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@android:color/white"/>
            <stroke
                android:width="20dp"
                android:color="@color/colorTransparent"
            />
            <corners
                android:radius="30dp"
                />
        </shape>
    </item>
</layer-list>
